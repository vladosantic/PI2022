@model PI2022.Models.Bidding

@{
    ViewData["Title"] = "Detalji natječaja";
}

<h1>Detalji natječaja</h1>
<hr/>
<div>
    <dl class="row">
        <dt class = "col-sm-2">
            Naziv natječaja
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Naziv)
        </dd>
        <dt class = "col-sm-2">
            Opis posla
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Opis)
        </dd>
        <dt class = "col-sm-2">
            Procijenjena vrijednost
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.ProcijenjenaVrijednost) KM
        </dd>
        <dt class = "col-sm-2">
            Trajanje natječaja
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Trajanje)
        </dd>
        <dt class = "col-sm-2">
            Objavljen
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Objavljen)
        </dd>
        <dt class = "col-sm-2">
            Dobitnik natječaja
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Dobitnik)
        </dd>
    </dl>
</div>
<div>
    <a asp-action="Edit" asp-route-id="@Model?.Id">Uredi</a> |
    <a asp-action="Index">Natrag na popis</a>
</div>
